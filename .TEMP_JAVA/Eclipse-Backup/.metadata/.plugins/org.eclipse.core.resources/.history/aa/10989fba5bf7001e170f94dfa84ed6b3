import java.util.Scanner;

public class PrimeiraClasse { 
	public static void main(String[] args) {
	Scanner t = new Scanner(System.in);
	
	while(true) {
	System.out.println("Nome: ");
	String a = t.nextLine();
	
	System.out.println("Numero: ");
	int b = t.nextInt();
	t.nextLine();
	
	System.out.printf("\n %s  -- %d", a, b);
	}
	}


}